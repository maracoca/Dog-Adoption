generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Dog {
  id          Int      @id @default(autoincrement())
  name        String
  breed       String
  age         Int
  qualities   String
  description String
  image       String
}

model User {
  id          Int     @id @default(autoincrement())
  name        String @db.VarChar(40)
  email       String @db.VarChar(40) @unique
  password    String @db.VarChar(255)
}
